class SnakeGame {

    field Snake s;
    constructor SnakeGame new(){
        return this;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
   	}

    method void run() {
        let s = Snake.new(4,4,4);
        while (true){
            do Sys.wait(1000);
            do s.move();
        }
        return;
    }
}
